[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    ds = diff --staged
    lg = log -p
    lgo = log --oneline
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all --date=local
    ls = ls-files
    fullci = commit --allow-empty -m '[full-ci]'
    gpg = config user.signingkey
[core]
    editor = vim
[credential]
    helper = osxkeychain
[filter "lfs"]
    clean = git lfs clean %f
    smudge = git lfs smudge %f
    required = true
[commit]
    gpgsign = true
[github]
    user = freaz
