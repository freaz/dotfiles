[core]
  editor = vim
[credential]
  helper = osxkeychain
[commit]
  gpgsign = true
[github]
  user = freaz
[pull]
  ff = only
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  ds = diff --staged
  lg = log -p
  lgo = log --oneline
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all --date=local
  ls = ls-files
  gpg = config user.signingkey

[init]
	defaultBranch = main
